{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "ES2022",
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "rootDir": "../",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
    },
    "include": ["src"]
}